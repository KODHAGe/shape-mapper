{
  "name": "shape-mapper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "wc": "vue-cli-service  build --target wc --name shape-mapper src/App.vue --dest dist/component/",
    "analyse": "vue-cli-service build --bundle-analyser",
    "now": "now ./dist -A ../now.json --public && now alias",
    "publish": "npm run build && npm run wc && npm run now"
  },
  "dependencies": {
    "@types/lodash.debounce": "^4.0.4",
    "firebase": "^5.4.2",
    "lodash": "^4.17.10",
    "p5": "^0.7.2",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-typescript": "^3.0.0",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "typescript-eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
